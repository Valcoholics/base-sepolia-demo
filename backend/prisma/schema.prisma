generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AP721 {
  ap721     String  @id
  sender    String?
  owner     String?
  logic     String?
  renderer  String?
  factory   String?
  createdAt BigInt?
  target    String?
}

model TokenStorage {
  ap721   String      @id
  tokenId TokenInfo[]
}

model TokenInfo {
  tokenId     Int     @id @default(autoincrement())
  pointer     String
  decodedData String
  sender      String
  metadata    String
  burned      Boolean
  updatedAt   String
  updatedBy   String
  TokenStorage      TokenStorage? @relation(fields: [tokenStorageAp721], references: [ap721])
  tokenStorageAp721 String?
}

model Transaction {
  id        String @id
  address   String
  eventType String
  tags      Json
}
