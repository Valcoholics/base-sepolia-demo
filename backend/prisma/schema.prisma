generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AP721 {
  ap721     String @id
  sender    String
  owner     String
  logic     String
  renderer  String
  factory   String
  createdAt BigInt
}

model TokenStorage {
  ap721     String
  tokenId   BigInt
  pointer   String
  updatedAt BigInt
  updatedBy String

  @@id([ap721, tokenId])
}

model Transaction {
  id        String @id
  address   String
  eventType String
  tags      Json
}
